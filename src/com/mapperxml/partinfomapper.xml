<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.PartInfoMapper">
		
	<select id="select" parameterType="String" resultType="com.vo.PartInfo">
		SELECT * FROM part_info WHERE part_id=#{part_id}
	</select>
	<select id="selectAll" resultType="com.vo.PartInfo">
		SELECT * FROM part_info
	
	</select>
	


</mapper>
